          "metadata": {
            "operationMetadataId": "02648909-16c8-44a4-9bba-1ff703fa23c9"
          },
          "type": "OpenApiConnectionWebhook",
          "inputs": {
            "host": {
              "connectionName": "shared_commondataserviceforapps",
              "operationId": "SubscribeWebhookTrigger",
              "apiId": "/providers/Microsoft.PowerApps/apis/shared_commondataserviceforapps"
            },
            "parameters": {
              "subscriptionRequest/message": 4,
              "subscriptionRequest/entityname": "bsr_biregcountry",
              "subscriptionRequest/scope": 4,
              "subscriptionRequest/filteringattributes": "bsr_countryid, bsr_biapplicationid,bsr_buildinginspectorid, bsr_account"
            },
            "authentication": "@parameters('$authentication')"
          }
        }
      },
      "actions": {
        "Get_a_Country_row_by_ID": {
          "runAfter": {
            "Condition": [
              "Succeeded"
            ]
          },
          "metadata": {
            "operationMetadataId": "f5c8b47c-4b0e-46fc-991c-4ea018306842"
          },
          "type": "OpenApiConnection",
          "inputs": {
            "host": {
              "connectionName": "shared_commondataserviceforapps",
              "operationId": "GetItem",
              "apiId": "/providers/Microsoft.PowerApps/apis/shared_commondataserviceforapps"
            },
            "parameters": {
              "entityName": "bsr_countries",
              "recordId": "@triggerOutputs()?['body/_bsr_countryid_value']"
            },
            "authentication": "@parameters('$authentication')"
          }
        },
        "Update_the_BI_Registration_Country_name_row": {
          "runAfter": {
            "Get_a_Country_row_by_ID": [
              "Succeeded"
            ]
          },
          "metadata": {
            "operationMetadataId": "e66dc379-3625-4079-a62a-dca1ff657309"
          },
          "type": "OpenApiConnection",
          "inputs": {
            "host": {
              "connectionName": "shared_commondataserviceforapps",
